{
  "name": "weather-station",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "ng": "ng",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && ng serve",
    "install": "cd client && npm install",
    "build": "cd client && ng build",
    "migrate": "knex migrate:latest",
    "unmigrate": "knex migrate:rollback",
    "seed": "knex seed:run",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@angular/cli": "~9.0.3",
    "@angular/compiler-cli": "~9.0.2",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.1.0",
    "connect-redis": "^4.0.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.5",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.2",
    "redis": "^3.0.2",
    "uuid": "^7.0.2",
    "validator": "^13.0.0"
  }
}
